<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.metodista.pap.ssm">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.metodista.pap.ssm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NovaContaActivity"></activity>
        <activity android:name=".IndexActivity"></activity>
        <activity android:name=".NovaTemporadaActivity"></activity>
        <activity android:name=".EntrarTemporadaActivity"></activity>
        <activity android:name=".NovoRankingActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>